{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_df_sql_emp",
						"type": "DatasetReference"
					},
					"name": "SourceEmployee"
				},
				{
					"dataset": {
						"referenceName": "ds_df_deptno",
						"type": "DatasetReference"
					},
					"name": "SourceDeptNo"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "RenamedDeptNotoDDeptNo"
				},
				{
					"name": "join1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ENO as integer,",
				"          ENAME as string,",
				"          JOB as string,",
				"          COMPID as integer,",
				"          DOJ as timestamp,",
				"          SAL as integer,",
				"          COMM as integer,",
				"          DEPTNO as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SourceEmployee",
				"source(output(",
				"          DEPTNO as integer,",
				"          DNAME as string,",
				"          LOCATION as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SourceDeptNo",
				"SourceDeptNo select(mapColumn(",
				"          D_DEPTNO = DEPTNO,",
				"          DNAME,",
				"          LOCATION",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> RenamedDeptNotoDDeptNo",
				"SourceEmployee, RenamedDeptNotoDDeptNo join(SourceEmployee@DEPTNO == D_DEPTNO,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1"
			]
		}
	}
}