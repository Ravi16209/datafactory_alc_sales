{
	"name": "pl_json_testing",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['returnReference']"
								},
								"sink": {
									"name": "returnReference",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['orderReference']"
								},
								"sink": {
									"name": "orderReference",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['customerId']"
								},
								"sink": {
									"name": "customerId",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['timestamp']"
								},
								"sink": {
									"name": "timestamp",
									"type": "String"
								}
							}
						],
						"collectionReference": ""
					}
				},
				"inputs": [
					{
						"referenceName": "ds_Json_testing",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ds_sql",
						"type": "DatasetReference",
						"parameters": {
							"schemaname": "MAIN",
							"tablename": "dummyt"
						}
					}
				]
			}
		],
		"annotations": []
	}
}